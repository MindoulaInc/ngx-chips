{
  "name": "ngx-chips",
  "version": "16.0.0",
  "description": "Tag Input component for Angular",
  "scripts": {
    "release": "npm run build && npm publish dist",
    "postrelease": "rm -rf dist",
    "build": "ng-packagr -p ./ng-packager.json && mkdirp ./dist/dist && ncp ./modules/core/styles ./dist/core/styles",
    "server": "ng serve --port 3010",
    "start": "npm run server",
    "test": "ng test modules --browsers=ChromeHeadless --watch=false",
    "codecov": "ng test modules --code-coverage --browsers=ChromeHeadless --watch=false",
    "ng": "ng",
    "prepare": "npm run build"
  },
  "author": {
    "name": "Giancarlo Buomprisco",
    "email": "giancarlopsk@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/animations": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.37",
    "jasmine-core": "5.1.1",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "protractor": "~7.0.0",
    "rxjs": "7.4.0",
    "tslint": "~6.1.3",
    "typescript": "4.8.4",
    "zone.js": "~0.11.4"
  },
  "dependencies": {
    "ng2-material-dropdown": ">=1.0.0"
  },
  "peerDependencies": {},
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/angular@0.2.0"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
